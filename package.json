{
  "name": "umalator-global-cli",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsx build.ts",
    "build:css": "node node_modules/@tailwindcss/cli/dist/index.mjs -i ./public/input.css -o ./public/style.css --minify",
    "build:frontend": "vite build",
    "start": "node cli.js",
    "web": "npm run build:css && npm run build:frontend && tsx server.ts",
    "web:watch": "node node_modules/@tailwindcss/cli/dist/index.mjs -i ./public/input.css -o ./public/style.css --watch",
    "dev": "concurrently \"npm run web:watch\" \"tsx server.ts\" \"vite\""
  },
  "dependencies": {
    "commander": "^11.0.0",
    "esbuild": "^0.27.2",
    "express": "^4.18.0",
    "immutable": "^4.3.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "vite": "^7.3.0"
  }
}
